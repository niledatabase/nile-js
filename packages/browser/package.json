{
  "name": "@theniledev/browser",
  "version": "0.38.2",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.0"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "yarn build:move && yarn build:api:gen && tsdx build --tsconfig ./tsconfig.build.json",
    "build:move": "node ./scripts/move.mjs",
    "build:api:gen": "yarn openapi-generator-cli generate --skip-validate-spec -t templates -i src/spec.json -g typescript-fetch --package-name @theniledev/client -o src/openapi --additional-properties=ngVersion=6.1.7,npmName=theniledev,supportsES6=true,npmVersion=6.9.0,withInterfaces=true,moduleName=NileClient,typescriptThreePlus=true,projectName=@theniledev/client",
    "test": "tsdx test",
    "lint": "eslint src",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "jrea",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheNileDev/nile-js.git",
    "directory": "packages/browser"
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "dist/browser.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/browser.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@babel/core": "^7.20.12",
    "@typescript-eslint/parser": "^5.49.0",
    "babel-loader": "^9.1.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "size-limit": "^8.1.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0"
  }
}
