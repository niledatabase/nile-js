{
  "name": "@niledatabase/react",
  "version": "4.0.0-alpha.21",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "style": "./dist/styles.css",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build:storybook": "yarn build:tailwind && storybook build",
    "build:tailwind": "npx @tailwindcss/cli -i ./.storybook/globals.css -o ./.storybook/output.css",
    "build": "npx @tailwindcss/cli -i ./.storybook/globals.css -o ./dist/styles.css --minify && tsup src/index.ts",
    "dev:local": "tsup src/index.ts --watch",
    "dev:tailwind": "npx @tailwindcss/cli -i ./.storybook/globals.css -o ./.storybook/output.css --watch",
    "dev": "storybook dev",
    "lint": "eslint src",
    "size": "size-limit",
    "storybook": "yarn build:tailwind && CACHE_DIR=$PWD/.yarn/cache storybook dev -p 6006",
    "test": "yarn jest"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.23.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "jrea",
  "repository": {
    "type": "git",
    "url": "https://github.com/niledatabase/nile-js.git",
    "directory": "packages/react"
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "dist/react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@chromatic-com/storybook": "^1.9.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@size-limit/preset-small-lib": "^8.2.6",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-onboarding": "8.0.4",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/addon-themes": "^8.6.7",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.5",
    "@storybook/blocks": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-webpack5": "^8.3.4",
    "@storybook/test": "^8.0.0",
    "@storybook/theming": "^8.0.4",
    "@svgr/rollup": "^8.1.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.9",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.4.20",
    "babel-jest": "29.7.0",
    "babel-loader": "^9.1.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dts-cli": "^2.0.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.47",
    "prop-types": "^15.8.1",
    "rollup-plugin-terser": "^7.0.2",
    "size-limit": "^8.2.6",
    "storybook": "^8.0.0",
    "tailwind": "^4.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.15",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "tsup": "^8.3.0",
    "tw-animate-css": "^1.2.4",
    "typescript": "^5.3.2",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@niledatabase/browser": "^4.0.0-alpha.21",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tanstack/react-query": "^5.23.0",
    "lucide-react": "^0.447.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.53.0",
    "react-is": "^19.0.0"
  }
}
